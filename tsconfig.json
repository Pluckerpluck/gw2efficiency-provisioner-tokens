{
    "compilerOptions": {
        "module": "es2015",
        "target": "es5",
        "removeComments": true,
        "noResolve": false,
        "sourceMap": true,
        "noImplicitAny": true,
        "noEmitOnError": true
    },
    "compileOnSave": false,
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts"
    ]
}